<!DOCTYPE html>
<html>
<head>
	<title>Display different iframe based on button press</title>
	<style>
		iframe {
			width: 100%;
			height: 500px;
		}
	</style>
</head>
<body>
	<h1>Display different iframe based on button press</h1>
	<p>Click on one of the buttons below to display a different iframe:</p>
	<button id="button1">Iframe 1</button>
	<button id="button2">Iframe 2</button>
	<button id="button3">Iframe 3</button>
	<div id="iframe-container"></div>
	<script>
		// Get references to the buttons and iframe container
		const button1 = document.getElementById("button1");
		const button2 = document.getElementById("button2");
		const button3 = document.getElementById("button3");
		const iframeContainer = document.getElementById("iframe-container");
		
		// Set up event listeners for each button
		button1.addEventListener("click", function() {
			iframeContainer.innerHTML = '<iframe src="https://script.google.com/macros/s/AKfycbz5M7nrkqHQ-2uC-5ebJqBQDDUeCyxS7b5EAP1sBCw4btOkfN26l3p_Z8UhQxWEFmj-tQ/exec"></iframe>';
		});
		button2.addEventListener("click", function() {
			iframeContainer.innerHTML = '<iframe src="https://www.example.com/iframe2"></iframe>';
		});
		button3.addEventListener("click", function() {
			iframeContainer.innerHTML = '<iframe src="https://www.example.com/iframe3"></iframe>';
		});
	</script>
</body>
</html>
